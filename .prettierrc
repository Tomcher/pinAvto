{
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": false,
    "bracketSpacing": true,
    "plugins": ["prettier-plugin-xml"],
    "xmlSelfClosingSpace": true,
    "xmlWhitespaceSensitivity": "ignore",
    "xmlSortAttributes": true,
    "xmlStrictSelfClosing": false,
    "xmlFormatterOptions": {
      "indentation": "  ",
      "collapseContent": false,
      "lineSeparator": "\n",
      "formatSelfClosingTag": true
    }
  }
  